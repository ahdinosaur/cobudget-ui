{
  "name": "cobudget-ui",
  "version": "0.1.0",
  "description": "user interface for CoBudget!",
  "main": "./server.js",
  "browser": "src/client.coffee",
  "scripts": {
    "start": "gulp build && node .",
    "develop": "gulp develop",
    "test-client": "testling -x google-chrome",
    "test-server": "mocha -r src/coffee.js -R spec src/node_modules/**/test/*.coffee",
    "test": "(export NODE_ENV=test; npm run test-server && npm run test-client)",
    "dev": "(export NODE_ENV=development; npm run develop)",
    "prod": "(export NODE_ENV=production; npm run start)"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/open-app/cobudget-ui.git"
  },
  "keywords": [],
  "author": "open-app",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/open-app/cobudget-ui/issues"
  },
  "homepage": "https://github.com/open-app/cobudget-ui",
  "browserify": {
    "transform": [
      "coffee-reactify",
      "envify"
    ]
  },
  "testling": {
    "files": "src/node_modules/**/test/*.coffee",
    "harness": "mocha",
    "browsers": [
      "ie/9..latest",
      "firefox/15,18,21,23..nightly",
      "chrome/20,23,26,29..canary",
      "opera/15..next",
      "safari/6..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/4.2"
    ]
  },
  "dependencies": {
    "bluebird": "^2.3.2",
    "bootstrap": "^3.2.0",
    "browserify": "^5.12.0",
    "brstar": "^0.1.0",
    "bundle-collapser": "^1.1.0",
    "coffee-react": "^1.0.2",
    "coffee-reactify": "^1.0.2",
    "coffeeify": "^0.7.0",
    "debug": "^2.0.0",
    "ecstatic": "^0.5.4",
    "envify": "^3.0.0",
    "express": "^4.9.5",
    "extend": "^1.3.0",
    "fluxxor": "^1.4.1",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-less": "^1.3.6",
    "gulp-plumber": "^0.6.5",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.2.2",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "location-bar": "^2.0.0",
    "lodash": "^2.4.1",
    "mold-source-map": "^0.3.0",
    "nets": "^1.0.0",
    "react": "^0.11.2",
    "uuid": "^1.4.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "connect-livereload": "^0.4.0",
    "gulp-livereload": "^2.1.1",
    "mocha": "^1.21.4",
    "mocha-as-promised": "^2.0.0",
    "nodemon": "^1.2.1",
    "sinon": "^1.10.3",
    "testling": "^1.7.1",
    "tiny-lr": "^0.1.4",
    "watchify": "^1.0.6"
  }
}
